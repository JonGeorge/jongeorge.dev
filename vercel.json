{
  "rewrites": [
    {
      "source": "/.well-known/webfinger",
      "destination": "/.well-known/webfinger"
    },
    {
      "source": "/.well-known/README.md",
      "destination": "/404"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/webfinger",
      "headers": [
        { "key": "Content-Type", "value": "application/jrd+json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
